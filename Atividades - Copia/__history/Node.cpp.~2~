#include <vcl.h>
#include "Node.h"

//---------------------------------------------------------------------------
Node::Node()
{

}
//---------------------------------------------------------------------------
Node::Node(int a)
{
	this->a = a;
}
//---------------------------------------------------------------------------
Node::Node(int a, int b)
{
	this->a = a;
	this->b = b;
}
//---------------------------------------------------------------------------
void Node::calcularQuadrado()
{
	ShowMessage(String(pow(a,2)));
}
